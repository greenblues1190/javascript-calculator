{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "airbnb-base", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": ["off"],
    "no-alert": "off",
    "camelcase": "off",
    "class-methods-use-this": "off",
    "no-new": "off",
    "consistent-return": "off"
  }
}
